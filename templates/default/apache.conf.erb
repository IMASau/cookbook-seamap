  Alias <%= @approot %>/static/ <%= @appdir %>/current/backend/static/
  ProxyPass           <%= @approot %>/static !
  ProxyPass           <%= @approot %> http://localhost:8092 timeout=600 connectiontimeout=5
  ProxyPassReverse    <%= @approot %> http://localhost:8092
  <Directory <%= @appdir %>/current/backend/static>
    Order deny,allow
    Allow from all
  </Directory>
